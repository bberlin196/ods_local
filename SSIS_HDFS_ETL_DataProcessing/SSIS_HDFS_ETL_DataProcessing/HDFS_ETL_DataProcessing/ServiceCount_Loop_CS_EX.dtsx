        public void LogMessage(string message)
        {
            using (System.IO.StreamWriter file = new System.IO.StreamWriter(@"C:\HDFS_Stage\log\ServiceCount_Loop_CS.log", true))
            {
                file.WriteLine(String.Format("{0}`{1}", DateTime.Now.ToString(), message));
            }
        }


        const string sql_daily = @"
SET DATEFIRST 1;	
DECLARE @start DATE = '{0}'
DECLARE @end DATE = DATEADD(day,1,@start)
INSERT INTO [HDFS_Stage].[dbo].[{1}]
([servdef]
      ,[adds]
      ,[cancels]
      ,[beginning]
      ,[pclass]
      ,[class]
      ,[subclass]
      ,[ending]
      ,[w_ending]
      ,[m_ending]
      ,[entdate]
      ,[last_entdate]
      ,[req_date]
      ,[rec_datetime_stamp]
	  )
SELECT 
    c.servdef as servdef,
	c.adds AS adds,
	c.cancels cancels,
	c.beginning,
    sdm.pclass, 
	sdm.class class,
	IIF(c.servdef = 1089, 'SecPacks', sfdm.subclass) as subclass,
	c.ending,
	null as [w_ending],
	null as [m_ending],
	c.entdate,
	LAST_VALUE(c.entdate) OVER (PARTITION BY c.servdef, IIF(DATEPART(wk,c.entdate)=53,1,DATEPART(wk,c.entdate)) ORDER BY c.entdate ASC RANGE BETWEEN CURRENT ROW AND UNBOUNDED FOLLOWING ) AS last_entdate,
    c.startdate as req_date,
	GETDATE() as [rec_datetime_stamp]
FROM 	
	mtbiODS.[dbo].[rpt_servicerevenuechurn_fn](@start,@end,'day','service') c
LEFT JOIN 	
	mtbiODS.dbo.servdefmap_fn() sdm ON (sdm.servdef = c.servdef)
LEFT JOIN 	
	mtbiODS.dbo.servdefmap sfdm ON (sfdm.servdef = c.servdef)
";

