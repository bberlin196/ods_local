                                string.Format("ServiceCount_Loop_CS [{0}], SQL:[{1}]", e, sql), null, 0);
                                Dts.TaskResult = (int)DTSExecResult.Failure;
                            }

                            dStart = dStart.AddDays(1);

                        } // while (dStart < max_end)

                    } // using (sqlComm = new System.Data.SqlClient.SqlCommand())
                    cm.ReleaseConnection(sqlConn);

