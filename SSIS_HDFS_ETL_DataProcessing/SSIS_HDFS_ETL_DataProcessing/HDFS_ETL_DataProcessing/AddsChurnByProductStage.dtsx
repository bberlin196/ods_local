                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE @end DATE = GETDATE()
DECLARE @start DATE = '2015-01-01'
--select DATEDIFF(DAY,@start, @end)
--DECLARE @start DATE = DATEADD(day,-10,@end)
	
/* Set start of the week to Monday	*/
SET DATEFIRST 1;	

SELECT 
    c.servdef as servdef,
	SUM(c.beginning) beginning,
	SUM(c.adds) AS adds,
	SUM(c.cancels) cancels,
	SUM(c.ending) ending,
    sdm.pclass, 
	sdm.class class,
	IIF(c.servdef = 1089, 'SecPacks', sfdm.subclass) as subclass,
	c.entdate,
	LAST_VALUE(c.entdate) OVER (PARTITION BY c.servdef, IIF(DATEPART(wk,c.entdate)=53,1,DATEPART(wk,c.entdate)) ORDER BY c.entdate ASC RANGE BETWEEN CURRENT ROW AND UNBOUNDED FOLLOWING ) AS last_entdate,
    c.startdate as req_date
FROM 	
	mtbiODS.[dbo].[rpt_servicerevenuechurn_fn](@start,@end,'day','service') c
LEFT JOIN 	
	mtbiODS.dbo.servdefmap_fn() sdm ON (sdm.servdef = c.servdef)
LEFT JOIN 	
	mtbiODS.dbo.servdefmap sfdm ON (sfdm.servdef = c.servdef)
--WHERE c.servdef in (15,1354)
GROUP BY c.servdef, sdm.pclass, sdm.class, IIF(c.servdef = 1089, 'SecPacks', sfdm.subclass), c.entdate, c.startdate</property>
