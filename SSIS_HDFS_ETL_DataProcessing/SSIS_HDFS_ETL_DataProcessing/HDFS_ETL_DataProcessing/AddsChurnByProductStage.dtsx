        /// </summary>
        public void Main()
        {
            string fileIn = @"C:\HDFS_Stage\data\ServiceCountIn.txt";
            string fileOut = @"C:\HDFS_Stage\data\ServiceCount.txt";

            Encoding utf8WithoutBom = new System.Text.UTF8Encoding(false);
            Encoding un1 = new System.Text.UnicodeEncoding();

            try
            {
                using (StreamReader sR = new StreamReader(fileIn, un1))
                {
                    using (StreamWriter sW = new StreamWriter(fileOut, false, utf8WithoutBom))
                    {

                        while (sR.Peek() != -1) // stops when it reachs the end of the file
                        {
                            sW.WriteLine(sR.ReadLine()); // writing the edited line as a new line in the new.txt file
                        }
                    }
                }              

            }
            catch (Exception e)
            {
                Dts.Events.FireError(0, null,
                string.Format("Error {0} in UF8 Task Script using file: {1}", e,fileIn), null, 0);
