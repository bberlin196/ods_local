                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE @end DATE = GETDATE()
DECLARE @start DATE = '2015-01-01'


/* Set start of the week to Monday	*/
SET DATEFIRST 1;	

select   servdef,
	(CONVERT(VARCHAR(10), mm.req_start_date, 112) + CONVERT(VARCHAR(10), mm.req_end_date, 112)) as [dates_range],
	sum([ending]) ending
	from
(	
SELECT c. servdef,
    c.ending,										
	c.startdate as req_start_date,										
	c.enddate   as req_end_date										
FROM 											
	mtbiODS.[dbo].[rpt_servicerevenuechurn_fn](@start,@end,'month','service') c									
) mm
group by  servdef,
	(CONVERT(VARCHAR(10), mm.[req_start_date], 112) + CONVERT(VARCHAR(10), mm.[req_end_date], 112))</property>
