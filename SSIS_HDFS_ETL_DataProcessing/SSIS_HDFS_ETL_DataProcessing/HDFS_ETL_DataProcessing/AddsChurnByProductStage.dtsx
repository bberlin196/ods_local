                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE @end DATE = GETDATE()
DECLARE @start DATE = '2015-01-01'
--select DATEDIFF(DAY,@start, @end)
--DECLARE @start DATE = DATEADD(day,-10,@end)
	
/* Set start of the week to Monday	*/
SET DATEFIRST 1;	

select 
	res.servdef,
	res.beginning,
	res.adds,
	res.cancels,
	res.ending,
    res.pclass, 
	res.class,
	res.subclass,
	wk.[ending] as [w_ending],
	mm.[ending] as [m_ending],
	res.entdate,
	res.last_entdate,
    res.req_date,
	GETDATE() as [rec_datetime_stamp],
	NULL as rec_num

from (
SELECT 
    *
FROM [stage].[ServiceCount_Full_Load_PerDay]

) res
JOIN 											
(											
	select servdef, 	
        ending,
		convert(date,left([week_dates_range],8)) as [req_start_date],
		convert(date,right([week_dates_range],8)) as [req_end_date] 
	FROM 										
	    [stage].[ServiceCount_WK_GRP_D] 									
) wk on res.servdef = wk.servdef and res.req_date between wk.[req_start_date] and wk.[req_end_date]
JOIN 											
(											
	select 	servdef,
        ending,
		convert(date,left([dates_range],8)) as [req_start_date],
		convert(date,right([dates_range],8)) as [req_end_date] 
	FROM 										
	    [stage].[ServiceCount_MM_GRP_D]
) mm on  res.servdef = mm.servdef and  res.req_date between mm.[req_start_date] and mm.[req_end_date]</property>
