        static public bool IsFileReady(String sFilename)
        {
            // If the file can be opened for exclusive access it means that the file
            // is no longer locked by another process.
            try
            {
                if (!System.IO.File.Exists(sFilename)) return true;

                using (System.IO.FileStream inputStream = File.Open(sFilename, FileMode.Open, FileAccess.Write, FileShare.Write))
                {
                    if (inputStream.Length > 0)
                    {
                        return true;
                    }
                    else
                    {
                        return false;
                    }

                }
            }
            catch (Exception e)
            {
                Console.WriteLine("IsFileReady:{0}", e.Message);
                return false;
            }
        }


        static public void LogMessage(string message)
        {


            string logFile = @"C:\HDFS_Stage\log\ESOM_DataLoader.log";
            string log = logFile;

            // just wait
            if (!IsFileReady(logFile)) System.Threading.Thread.Sleep(5000);

            // ok tried diferent file
            if (!IsFileReady(logFile)) log = logFile + DateTime.Now.Ticks.ToString();

            FileStream fl = File.Open(log, FileMode.Append, FileAccess.Write, FileShare.ReadWrite);

            using (System.IO.StreamWriter file = new System.IO.StreamWriter(fl))
            {
                file.WriteLine(message);
            }

        }


        void ValidateSalesOrders(string salesOrdersPath)
        {
            try
            {
                if(!File.Exists(salesOrdersPath))
                    throw new Exception(string.Format("File: {0} is not found.",salesOrdersPath));

                string readText = File.ReadAllText(salesOrdersPath);

                //System.Web.Script.Serialization.JavaScriptSerializer ser = new System.Web.Serialization.JavaScriptSerializer();
                //Dictionary dict = ser.Deserialize<Dictionary<string, object>>(input);

                //string schemaJson = "";
                //JsonSchema schema = JsonSchema.Parse(schemaJson);
            }
            catch (Exception ex)
            {

                ScriptMain.LogMessage(String.Format("{0}`{1}::Run_cURL`[{2}]", DateTime.Now.ToString(), varPackageName, ex.Message));
                throw ex;
            }
        
        
        }

        private bool Run_cURL()
        {
            /* curl -k -E devesomevo.authclient.int.dev-godaddy.com.pem --pass Putter -X POST "https://esomevoapi.acctgfn.int.dev-godaddy.com/v0/ESOMEvolution" 
              -H "Content-Type: application/json" -H "Accept: application/json" -d  @salesorders.json
             * */
            
            string inFilePath = String.Format("{0}\\{1}", varDataDirectory, varJsonFile);
            if (!File.Exists(inFilePath))
                throw new Exception(string.Format("File: {0} is not found.", inFilePath));

            string curlLog = String.Format("{0}","C:\\HDFS_Stage\\log\\esom.curl.log");

            if (!File.Exists(varApiAuthFile))
                throw new Exception(string.Format("File: {0} is not found.", varApiAuthFile));


            string sArgs = String.Format(" -k -E {0} --pass {1} -X POST \"{2}\" -H \"Content-Type: application/json\" -H \"Accept: application/json\" -d  @\"{3}\" -o {4} ",
                   varApiAuthFile,
                   varApiToken,
                   varApiEndPointUrl,
                   inFilePath,
                   curlLog
                  );

            int exitCode = -1;
            try
            {

                ValidateSalesOrders(inFilePath);

                // Prepare the process to run
                ProcessStartInfo start = new ProcessStartInfo();
                // Enter in the command line arguments, everything you would enter after the executable name itself
                start.Arguments = sArgs;
                // Enter the executable to run, including the complete path
                start.FileName = varCurlExe;
                // Do you want to show a console window?
                start.WindowStyle = ProcessWindowStyle.Hidden;
                start.CreateNoWindow = true;

                // Redirect the error stream of the child process.
                start.UseShellExecute = false;
                start.RedirectStandardError = true;
                // Do not wait for the child process to exit before
                // reading to the end of its redirected error stream.
                // p.WaitForExit();
                // Read the error stream first and then wait.
                

                // Run the external process & wait for it to finish
                using (Process proc = Process.Start(start))
                {
                                        
                    string error = proc.StandardError.ReadToEnd();
                    proc.WaitForExit();

                    // Retrieve the app's exit code
                    exitCode = proc.ExitCode;

                    
                    if (exitCode != 0 || IsError(File.ReadAllText(curlLog)))
                    {
                        exitCode = exitCode == 0 ? -1 : exitCode;
                        lastCurlErrr = String.Format("CURL Error exitCode={0} Message: Log: {1} Output: {2}",exitCode, File.ReadAllText(curlLog) , error);   
                    }
                }
            

            }
            catch (Exception ex)
            {

                ScriptMain.LogMessage(String.Format("{0}`{1}::Run_cURL`[{2}]", DateTime.Now.ToString(), varPackageName, ex.Message));
                throw ex;
            }


            return exitCode == 0;
        }

        private bool IsError(string logText)
        {
           bool res = false;

           if (!String.IsNullOrEmpty(logText))
           {
               res = ((logText.ToUpper().IndexOf("ERROR") != -1) || (logText.ToUpper().IndexOf("INVALID") != -1));
           }

           return res;
        }

