        public void Main()
        {

            varPackageName = "ESOM Data Uploader";  // This is the default name which will be overwritten by System::PackageName

            try
            {
                // Init variables
                Init();

                int cur_iter = 1;
                int sleepTime = 1000 * 60 * varSleepTimeMin;
                
                
                if (varModuleToCall == 0)
                {
                    while (cur_iter <= varMaxIterNumber)
                    {
                        
                        if ( Run_cURL() )
                        {
                            // all Ok, just exit
                            break;
                        }
                        else
                        {
                            // Exit with Error
                            if(cur_iter + 1 > varMaxIterNumber)
                            {
                                throw new Exception(String.Format("Run_cURL Error {0}", lastCurlErrr));
                            }

                            // wait 5 min and repeat and go to the next iteration.
                            System.Threading.Thread.Sleep(sleepTime);
                            cur_iter++;
                        }

                    }

                    
                }
                                

                Dts.TaskResult = (int)ScriptResults.Success;
                
            }
            catch (Exception e)
            {
                Dts.TaskResult = (int)ScriptResults.Failure;

                Console.WriteLine("Exception {0}", e.Message);

                // transfer the error to the caller
                Dts.Variables["User::var_API_Call_ErrorMessage"].Value = e.Message;

                ScriptMain.LogMessage(String.Format("{0}`{1}::Main` [{2}]", DateTime.Now.ToString(), varPackageName, e.Message));

                Dts.TaskResult = (int)ScriptResults.Failure;
                
            }

            
        }

