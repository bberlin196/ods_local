                varPackageName = String.Format(@"{0}", Dts.Variables["PackageName"].Value);
                varApiToken = String.Format(@"{0}", Dts.Variables["varApiToken"].Value);
                varApiEndPointUrl = String.Format(@"{0}", Dts.Variables["varApiEndPointUrl"].Value);
