		/// <summary>
        /// This method is called when this script task executes in the control flow.
        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>
        /// 

		public void Main()
		{
			// TODO: Add your code here
            try
            {
                string dir = @"C:\HDFS_Stage\archive";
                List<string> lstFiles = Directory.GetFiles(dir, "*.zip", SearchOption.AllDirectories).ToList();

                foreach (string fName in lstFiles)
                {
                    FileInfo fi = new FileInfo(fName);
                    if ( File.GetCreationTime(fName).Date == DateTime.Now.Date
                        &&
                        fi.Length / 1000.00 >= 900000.00
                        )
                    {
                        string message = String.Format("The large file of more than 900MB is found in Archive. The size of {0} is {1} bytes.", fi.Name, fi.Length);
                        Console.WriteLine(message);
                        using (System.IO.StreamWriter file = new System.IO.StreamWriter(@"C:\HDFS_Stage\log\HDFS_File_Audit.log", true))
                        {
                            file.WriteLine(String.Format("{0}`{1}", DateTime.Now.ToString(), message));
                            /* notify the caller */
                            throw new Exception(message);
                        }

                        /*string message1 = String.Format("File: {0} , Size: {1} , calc:[{2}], date:[{3}], date now:[{4}] ", fi.Name, fi.Length, fi.Length / 1000.00, File.GetCreationTime(fi.Name).Date, DateTime.Now.Date);
                        Console.WriteLine(message1);
                        using (System.IO.StreamWriter file = new System.IO.StreamWriter(@"C:\HDFS_Stage\log\HDFS_File_Audit.log", true))
                        {
                            file.WriteLine(String.Format("{0}`{1}", DateTime.Now.ToString(), message1));

                        }*/
                    }

                }
                                                
            }
            catch (Exception e)
            {
                Dts.TaskResult = (int)ScriptResults.Failure;

                Console.WriteLine("Exception {0}", e.Message);

                using (System.IO.StreamWriter file = new System.IO.StreamWriter(@"C:\HDFS_Stage\log\HDFS_File_Audit.log", true))
                {
                    file.WriteLine(String.Format("{0}`{1}", DateTime.Now.ToString(),e.Message));
                }
                throw e;
                
                //return;
            }

			Dts.TaskResult = (int)ScriptResults.Success;
		}

        #region ScriptResults declaration
        /// <summary>
