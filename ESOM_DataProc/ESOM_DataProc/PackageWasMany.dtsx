            Encoding="UTF8"><![CDATA[DECLARE @Recipients varchar(256) = 'bberlin@mediatemple.net;';
DECLARE @wmessage NVARCHAR(2000);

set @wsubject = 'RM SSIS HDFS ETL [Warning] SSIS PROD Master Presentation ETL seems to be delayed';
