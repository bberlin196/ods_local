        static public bool IsFileReady(String sFilename)
        {
            // If the file can be opened for exclusive access it means that the file
            // is no longer locked by another process.
            try
            {
                if (!System.IO.File.Exists(sFilename)) return true;

                using (System.IO.FileStream inputStream = File.Open(sFilename, FileMode.Open, FileAccess.Write, FileShare.Write))
                {
                    if (inputStream.Length > 0)
                    {
                        return true;
                    }
                    else
                    {
                        return false;
                    }

                }
            }
            catch (Exception e)
            {
                Console.WriteLine("IsFileReady:{0}", e.Message);
                return false;
            }
        }


        static public void LogMessage(string message)
        {


            string logFile = @"C:\HDFS_Stage\log\DW_Promoter.log";
            string log = logFile;

            // just wait
            if (!IsFileReady(logFile)) System.Threading.Thread.Sleep(5000);

            // ok tried diferent file
            if (!IsFileReady(logFile)) log = logFile + DateTime.Now.Ticks.ToString();

            FileStream fl = File.Open(log, FileMode.Append, FileAccess.Write, FileShare.ReadWrite);

            using (System.IO.StreamWriter file = new System.IO.StreamWriter(fl))
            {
                file.WriteLine(message);
            }

        }

        public bool DownloadCampaigns()
        {
            bool bResult = false;
            const string campaignTag = @"campaigns/";
            try
            {

                // Is it a full load?
                string sGet = "";

                if (varEndDate.Date == DateTime.Now.Date)
                {
                    // GET https://app.promoter.io/api/campaigns/
                    sGet = String.Format("{0}{1}",
                                            varApiUrl,
                                            campaignTag
                                            );
                    ScriptMain.LogMessage(String.Format("{0}`DW_Promoter::DownloadCampaigns`Full Load [current Date:{1}]", DateTime.Now.ToString(), DateTime.Now));
                }
                else
                {
                    DateTime startDate = varEndDate.AddDays(varDaysBack * -1);
                    // "https://app.promoter.io/api/campaigns/?posted_date_0=2017-12-11&posted_date_1=2018-01-10"
                    sGet = String.Format("{0}{1}?posted_date_0={2}&posted_date_1={3}",
                        varApiUrl,
                        campaignTag,
                        startDate.ToString("yyyy-MM-dd"),  /*2017-12-31*/
                        varEndDate.ToString("yyyy-MM-dd")
                        );
                    ScriptMain.LogMessage(String.Format("{0}`DW_Promoter::DownloadCampaigns`[startDate:{1} endDate:{2}]", DateTime.Now.ToString(), startDate.ToString("yyyy-MM-dd"), varEndDate.ToString("yyyy-MM-dd")));
                }





                int itet_num = 1;
                Campaigns cmp = new Campaigns();
                do
                {
                    string sJsonFile = String.Format(@"{0}\{1}.{2}.json",
                        varDataDirectory,
                        varCampaignsJsonFile,
                        itet_num);

                    if (File.Exists(sJsonFile))
                    {
                        File.Delete(sJsonFile);
                    }

                    Run_cURL(sGet, sJsonFile);

                    sGet = cmp.LoadCampaigns(sJsonFile);

                    itet_num++;

                } while (!String.IsNullOrEmpty(sGet));

                // Save the result set
                string sDataFile = String.Format(@"{0}\{1}",
                        varDataDirectory,
                        varCampaignsDbDataFile);
                if (File.Exists(sDataFile))
                {
                    File.Delete(sDataFile);
                }
                cmp.SaveDataAsFile(sDataFile);
                bResult = true;
            }
            catch (Exception ex)
            {
                // transfer the error to the caller
                Dts.Variables["User::var_API_Call_ErrorMessage"].Value = ex.Message;

                ScriptMain.LogMessage(String.Format("{0}`DW_Promoter::DownloadCampaigns`[{1}]", DateTime.Now.ToString(), ex.Message));
                bResult = false;
            }

            return bResult;
        }
        public bool DownloadAllFeedbacks()
        {
            bool bResult = false;

            try
            {
                
                // Is it a full load?
                string sGet = "";

                if ( varEndDate.Date == DateTime.Now.Date)
                {
                    // GET https://app.promoter.io/api/feedback/
                    sGet = String.Format("{0}{1}",
                                            varApiUrl,
                                            varFeedback
                                            );
                    ScriptMain.LogMessage(String.Format("{0}`DW_Promoter::DownloadAllFeedbacks`Full Load [current Date:{1}]", DateTime.Now.ToString(), DateTime.Now));
                }
                else
                {
                    DateTime startDate = varEndDate.AddDays(varDaysBack * -1);
                    // "https://app.promoter.io/api/feedback/?posted_date_0=2017-12-11&posted_date_1=2018-01-10"
                    sGet = String.Format("{0}{1}?posted_date_0={2}&posted_date_1={3}",
                        varApiUrl,
                        varFeedback,
                        startDate.ToString("yyyy-MM-dd"),  /*2017-12-31*/
                        varEndDate.ToString("yyyy-MM-dd")
                        );
                    ScriptMain.LogMessage(String.Format("{0}`DW_Promoter::DownloadAllFeedbacks`[startDate:{1} endDate:{2}]", DateTime.Now.ToString(), startDate.ToString("yyyy-MM-dd"), varEndDate.ToString("yyyy-MM-dd")));
                }



                

                int itet_num = 1;
                Feedbacks fb = new Feedbacks();
                do
                {
                    string sJsonFile = String.Format(@"{0}\{1}.{2}.json",
