    public class Feedbacks : PromoterData
    {
        public int count { get; set; }
        public string next { get; set; }
        public string previous { get; set; }

        private Dictionary<int, Feedback> dictFeedbacks;


        public void SaveDataAsFile(string sFile)
        {
            try
            {
                using (StreamWriter file = new StreamWriter(sFile))
                {
                    string record = String.Format("{0}`{1}`{2}`{3}`{4}`{5}`{6}",
                            "id",
                            "campaign",
                            "posted_date",
                            "score",
                            "score_type",
                            "contact.id",
                            "contact.email");
                    file.WriteLine(record);

                    foreach (KeyValuePair<int, Feedback> entry in dictFeedbacks)
                    {
                        record = String.Format("{0}`{1}`{2}`{3}`{4}`{5}`{6}",
                            entry.Value.id,
                            entry.Value.campaign,
                            entry.Value.posted_date,
                            entry.Value.score,
                            entry.Value.score_type,
                            entry.Value.contact.id,
                            entry.Value.contact.email);
                        file.WriteLine(record);
                    }
                }
            }
            catch (Exception ex)
            {

                ScriptMain.LogMessage(String.Format("{0}`DW_Promoter::Feedbacks::SaveDataAsFile`[{1}]", DateTime.Now.ToString(), ex.Message));
                throw ex;
            }

        }

        public string LoadFeedbacks(string path)
        {
            try
            {
                string sData = File.ReadAllText(path);

                DataValidation(sData);

                JObject rss = JObject.Parse(sData);

                count = (int)rss["count"];

                if (dictFeedbacks == null)
                    dictFeedbacks = new Dictionary<int, Feedback>(count);

                next = (string)rss["next"];
                previous = (string)rss["previous"];

                JArray categories = (JArray)rss["results"];
                foreach (JObject jFeedback in categories)
                {
                    Feedback fb = new Feedback();
                    fb.id = (int)jFeedback["id"];
                    //["contact"]["id"]
                    fb.contact = new Contact();
                    fb.contact.id = (int)jFeedback["contact"]["id"];
                    fb.contact.email = (string)jFeedback["contact"]["email"];
                    fb.campaign = (string)jFeedback["campaign"];
                    fb.score = (int)jFeedback["score"];
                    fb.score_type = (string)jFeedback["score_type"];
                    fb.posted_date = (string)jFeedback["posted_date"];

                    dictFeedbacks[dictFeedbacks.Count] = fb;
                }


            }
            catch (Exception ex)
            {

                ScriptMain.LogMessage(String.Format("{0}`DW_Promoter::Feedbacks::LoadFeedbacks`[{1}]", DateTime.Now.ToString(), ex.Message));
                throw ex;
            }

            return next;
        }

        

