        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>
        public void Main()
        {

            

            try
            {
                // Init variables
                Init();

                int cur_iter = 1;
                int sleepTime = 1000 * 60 * varSleepTimeMin;
                bool bResult = false;

                // Get Campaigns first if varModuleToCall is not specified 
                if (varModuleToCall == 0 || varModuleToCall == 1)
                {
                    while (cur_iter <= varMaxIterNumber)
                    {

                        if ((bResult =  DownloadCampaigns()))
                        {
                            // all Ok, just exit
                            break;
                        }
                        else
                        {
                            // wait 5 min and repeat and go to the next iteration.
                            System.Threading.Thread.Sleep(sleepTime);
                            cur_iter++;
                        }

                    }

                    if (!bResult)
                        throw new Exception("DW_Promoter::Main::DownloadCampaigns Failed");
                }

                cur_iter = 1;
                bResult = false;
                if (varModuleToCall == 0 || varModuleToCall == 2)
                {
                    while (cur_iter <= varMaxIterNumber)
                    {
                        if ((bResult = DownloadAllFeedbacks()))
                        {
                            // all Ok, just exit
                            break;
                        }
                        else
                        {
                            // wait 5 min and repeat and go to the next iteration.
                            System.Threading.Thread.Sleep(sleepTime);
                            cur_iter++;
                        }

                    }

                    if (!bResult)
                        throw new Exception("DW_Promoter::Main::DownloadCampaigns Failed");
                }

                Dts.TaskResult = (int)ScriptResults.Success;
                
            }
            catch (Exception e)
            {
                Dts.TaskResult = (int)ScriptResults.Failure;

                Console.WriteLine("Exception {0}", e.Message);

                ScriptMain.LogMessage(String.Format("{0}`DW_Promoter`[{1}]", DateTime.Now.ToString(), e.Message));

                Dts.TaskResult = (int)ScriptResults.Failure;

                // transfer the error to the caller
                Dts.Variables["User::var_API_Call_ErrorMessage"].Value = e.Message;

            }

            
        }

