                if (File.Exists(sDataFile))
                {
                    File.Delete(sDataFile);
                }
                fb.SaveDataAsFile(sDataFile);
                bResult = true;
            }
            catch (Exception ex)
            {
                // transfer the error to the caller
                Dts.Variables["User::var_API_Call_ErrorMessage"].Value = ex.Message;

                ScriptMain.LogMessage(String.Format("{0}`DW_Promoter::DownloadAllFeedbacks`[{1}]", DateTime.Now.ToString(), ex.Message));
                bResult = false;
            }

            return bResult;
        }


        /* When this call is made, it will return 100 results at a time and will require paginating through the results.
         * To paginate, you would add a page parameter to the url. Ex: <api_call>/api/feedback/?page=3
         */
        private int Run_cURL(string sGet, string sJsonFile)
        {
            string sArgs = String.Empty;

            if (string.IsNullOrEmpty(proxyURI))
            {

                sArgs = String.Format(" -k -X GET \"{0}\" -H \"Authorization: Token {1}\"   -o  {2}  ",
                   sGet,
                   varApiToken,
                   sJsonFile
                   );
            }
            else
            {
                sArgs = String.Format(" --proxy {0} -k -X GET \"{1}\" -H \"Authorization: Token {2}\"   -o  {3}  ",
                   proxyURI,
                   sGet,
                   varApiToken,
                   sJsonFile
                   );
            
            }

            
            int exitCode = -1;
            try
            {
                // Prepare the process to run
                ProcessStartInfo start = new ProcessStartInfo();
                // Enter in the command line arguments, everything you would enter after the executable name itself
                start.Arguments = sArgs;
                // Enter the executable to run, including the complete path
                start.FileName = varCurlExe;
                // Do you want to show a console window?
                start.WindowStyle = ProcessWindowStyle.Hidden;
                start.CreateNoWindow = true;

                // Redirect the error stream of the child process.
                start.UseShellExecute = false;
                start.RedirectStandardError = true;
                // Do not wait for the child process to exit before
                // reading to the end of its redirected error stream.
                // p.WaitForExit();
                // Read the error stream first and then wait.

                


                // Run the external process & wait for it to finish
                using (Process proc = Process.Start(start))
                {

                    
                    string error = proc.StandardError.ReadToEnd();
                    proc.WaitForExit();

                    // Retrieve the app's exit code
                    exitCode = proc.ExitCode;
                    if (exitCode != 0) throw new Exception(String.Format("Invalid Exit Code:{0}; error:{1}", exitCode,error));
                }


            }
            catch (Exception ex)
            {

                ScriptMain.LogMessage(String.Format("{0}`DW_Promoter::Run_cURL`[{1}]", DateTime.Now.ToString(), ex.Message));
                throw ex;
            }


            return exitCode;
        }

