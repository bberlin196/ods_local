                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select 
  bu_account.account,
  nps_set.nps_date,
  nps_set.nps_score,
  nps_set.nps_score_type

from
(

select distinct
  nps_all.[contact_email],
  nps_all.nps_date,
  nps_all.nps_score,
  nps_all.nps_score_type,
  nps_all.npsf_rn
from (

select 
  nps_res.[contact_email],
  nps_res.[posted_date] nps_date,
  npsf_scores.score_type nps_score_type,
  npsf_scores.score nps_score,
  npsf_rn = ROW_NUMBER() over ( Partition By
				nps_res.[contact_email]
				order by nps_res.[posted_date] desc)
from 
(  
SELECT 
       npsf.[contact_email],
       npsf.[posted_date], 
	   count(*) npsf_count
FROM [dbo].[promoterNPSfeedback] npsf
where npsf.[campaign] = 22778  
GROUP BY npsf.[contact_email],npsf.[posted_date] 
) nps_res
inner join [dbo].[promoterNPSfeedback] npsf_scores on nps_res.[contact_email] = npsf_scores.[contact_email]
) nps_all
where nps_all.npsf_rn = 1  -- and nps_all.[contact_email] = '99thbone@gmail.com'
) nps_set
inner join 
(
select 
				bu_res.email,
				bu_res.account,
				bu_res.startdate
		from
				(
		/*  Get Billing users */
		select 
				ltrim(rtrim(bu.email)) email,
  				bu.account,
				bu.startdate,
				user_rn = ROW_NUMBER() over ( Partition By bu.email,
				bu.account order by  bu.startdate desc )
				
			from HostOps.dbo.billingUser bu
			where
	    
